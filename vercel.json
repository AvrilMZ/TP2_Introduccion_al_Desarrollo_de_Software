{
    "version": 2,
    "routes": [
        {
            "src": "/css/(.*)",
            "dest": "/Frontend/css/$1"
        },
        {
            "src": "/js/(.*)",
            "dest": "/Frontend/js/$1"
        },
        {
            "src": "/img/(.*)",
            "dest": "/Frontend/img/$1"
        },
        {
            "src": "/html/(.*)",
            "dest": "/Frontend/html/$1"
        },
        {
            "src": "/",
            "dest": "/Frontend/html/index.html"
        }
    ]
}
