{
    "version": 2,
    "builds": [
        {
        "src": "Backend/package.json",
        "use": "@vercel/node"
        },
        {
        "src": "Frontend/**/*",
        "use": "@vercel/static"
        }
    ],
    "functions": {
        "Backend/index.js": {
        "runtime": "nodejs18.x"
        }
    },
    "env": {
        "DATABASE_URL": "@database_url",
        "DIRECT_URL": "@direct_url"
    }
}  
  